pwd
ls
cd workspaces
pwd
ls
cd ..
pwd
cd /default/default-domain/workspaces
pushd ..
popd

# Create an object (a folder), test commands on it
mkdir testdir
ls
id testdir
ls testdir
tree testdir
props testdir
propget testdir dc:title

# Now a file
cd testdir
mkfile testfile
ls
id testfile
ls testfile
props testfile
propget testfile dc:title
propset testfile dc:title some_other_title
propget testfile dc:title
setStream testfile run.sh
setStream testfile dummy.txt
cat testfile
rm testfile

# Put a file
put run.sh run.sh
put dummy.txt dummy.txt
#ls
