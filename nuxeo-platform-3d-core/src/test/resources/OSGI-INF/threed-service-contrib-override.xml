<?xml version="1.0"?>

<component name="org.nuxeo.ecm.platform.threed.service.ThreeDService.default.override">
    <documentation>
        Defines default override contributions for 3D service test
    </documentation>

    <extension target="org.nuxeo.ecm.platform.threed.service.ThreeDService"
               point="automaticLOD">
        <automaticLOD name="11" percentage="11" maxPoly="9999"
                      enabled="true" rendition="true" renditionVisible="false"/>
        <automaticLOD name="03" percentage="3" maxPoly="9999"
                      enabled="true" rendition="false"/>
        <automaticLOD name="53" percentage="53" maxPoly="9999"
                      enabled="true" rendition="false"/>
    </extension>

    <extension target="org.nuxeo.ecm.platform.threed.service.ThreeDService"
               point="renderViews">
        <renderView name="isometric" rendition="false"/>
    </extension>

    <extension target="org.nuxeo.ecm.platform.threed.service.ThreeDService"
               point="automaticRenderViews">
        <automaticRenderView name="top" enabled="false"/>
    </extension>
</component>
