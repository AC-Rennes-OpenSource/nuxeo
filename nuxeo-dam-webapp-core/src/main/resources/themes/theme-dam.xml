    </widget>
    <widget element="page[1]/section[1]/cell[2]/fragment[1]|page[3]/section[1]/cell[2]/fragment[1]">
      <view>dam banner</view>
    </widget>
    <widget element="page[1]/section[1]/cell[2]/fragment[2]|page[2]/section[1]/cell[2]/fragment[1]|page[3]/section[1]/cell[2]/fragment[2]">
      <view>dam includes</view>
    </widget>
    <widget element="page[1]/section[2]/cell[1]/fragment[1]">
      <view>dam search filters</view>
    </widget>
    <widget element="page[1]/section[2]/cell[2]/fragment[1]">
      <view>dam filtered search results</view>
    </widget>
    <widget element="page[1]/section[2]/cell[3]/fragment[1]">
      <view>dam selection view</view>
    </widget>
    <widget element="page[1]/section[3]/cell[1]/fragment[1]|page[3]/section[3]/cell[1]/fragment[1]">
      <view>dam footer</view>
    </widget>
    <widget element="page[2]/section[1]/cell[1]/fragment[1]|page[3]/section[2]/cell[1]/fragment[1]">
