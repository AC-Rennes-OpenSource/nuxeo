<?xml version="1.0" encoding="UTF-8"?>
<theme name="dam" template-engines="jsf-facelets">
  <layout>
    <page name="default">
      <section>
        <cell>
          <!-- dam logo -->
          <fragment type="generic fragment"/>
        </cell>
        <cell>
          <!-- dam banner -->
          <fragment type="generic fragment"/>
          <!-- Include -->
          <fragment type="generic fragment"/>
        </cell>
      </section>
      <section>
        <cell>
          <!-- dam search filters -->
          <fragment type="generic fragment"/>
        </cell>
        <cell>
          <!-- dam filtered search results -->
          <fragment type="generic fragment"/>
        </cell>
        <cell>
          <!-- main area -->
          <fragment type="generic fragment"/>
        </cell>
      </section>
      <section>
        <cell>
          <fragment type="generic fragment"/>
        </cell>
      </section>
    </page>
    <page name="popup">
      <section>
        <cell>
          <!-- main area -->
          <fragment type="region fragment" />
        </cell>
        <cell>
          <!-- Include -->
          <fragment type="generic fragment" />
        </cell>
      </section>
    </page>
    <page name="settings">
      <section>
        <cell>
          <!-- dam logo -->
          <fragment type="generic fragment"/>
        </cell>
        <cell>
          <!-- dam banner -->
          <fragment type="generic fragment"/>
          <!-- Include -->
          <fragment type="generic fragment"/>
        </cell>
      </section>
      <section>
        <cell>
          <!-- main area -->
          <fragment type="region fragment" />
        </cell>
      </section>
      <section>
        <cell>
          <fragment type="generic fragment"/>
        </cell>
      </section>
    </page>
  </layout>
  <properties element="page[2]/section[1]/cell[1]/fragment[1]">
    <name>body</name>
    <defaultBody />
    <defaultSrc />
  </properties>
  <properties element="page[3]/section[2]/cell[1]/fragment[1]">
    <name>body</name>
    <defaultBody />
    <defaultSrc />
  </properties>
  <formats>
