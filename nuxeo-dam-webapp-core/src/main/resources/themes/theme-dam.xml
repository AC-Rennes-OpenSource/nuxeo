<?xml version="1.0" encoding="UTF-8"?>
<theme name="dam" template-engines="jsf-facelets">
  <layout>
    <page name="default">
      <section>
        <cell>
          <!-- dam logo -->
          <fragment type="generic fragment"/>
        </cell>
        <cell>
          <!-- dam banner -->
          <fragment type="generic fragment"/>
          <!-- Include -->
          <fragment type="generic fragment"/>
        </cell>
      </section>
      <section>
        <cell>
          <!-- dam search filters -->
          <fragment type="generic fragment"/>
        </cell>
        <cell>
          <!-- dam filtered search results -->
          <fragment type="region fragment"/>
        </cell>
        <cell>
          <!-- main area -->
          <fragment type="generic fragment"/>
        </cell>
      </section>
      <section>
        <cell>
          <fragment type="generic fragment"/>
        </cell>
      </section>
    </page>
    <page name="popup">
      <section>
        <cell>
          <!-- main area -->
          <fragment type="region fragment" />
        </cell>
      </section>
    </page>
  </layout>
  <properties element="page[1]/section[2]/cell[2]/fragment[1]">
    <name>body</name>
    <defaultBody />
    <defaultSrc />
  </properties>
  <properties element="page[2]/section[1]/cell[1]/fragment[1]">
    <name>body</name>
    <defaultBody />
    <defaultSrc />
  </properties>
  <formats>
    <!-- LAYOUT -->
    <layout element="page[1]">
      <margin>0</margin>
      <padding>0</padding>
    </layout>
    <layout element="page[1]/section[1]">
      <width>100%</width>
      <height>36px</height>
      <overflow>auto</overflow>
    </layout>
    <layout element="page[1]/section[1]/cell[1]">
      <width>18%</width>
      <text-align>center</text-align>
      <vertical-align>middle</vertical-align>
    </layout>
    <layout element="page[1]/section[1]/cell[2]">
      <width>82%</width>
      <vertical-align>bottom</vertical-align>
    </layout>
    <layout element="page[1]/section[2]">
      <height>100%</height>
      <width>100%</width>
    </layout>
    <layout element="page[1]/section[2]/cell[1]">
      <width>18%</width>
      <padding>0px</padding>
    </layout>
    <layout element="page[1]/section[2]/cell[2]">
      <height>100%</height>
    </layout>
    <layout element="page[1]/section[2]/cell[3]">
      <width>410px</width>
    </layout>
    <layout element="page[1]/section[3]">
      <width>100%</width>
      <text-align>right</text-align>
    </layout>
    <layout element="page[1]/section[3]/cell[1]">
      <width>100%</width>
    </layout>
    <layout element="page[2]">
      <margin>0</margin>
      <padding>0</padding>
    </layout>
    <layout element="page[2]/section[1]">
      <width>100%</width>
    </layout>
    <layout element="page[2]/section[1]/cell[1]"/>
    <widget element="">
      <view>theme view</view>
      <icon>${org.nuxeo.ecm.contextPath}/icons/favicon.png</icon>
      <charset>utf-8</charset>
    </widget>
    <!-- WIDGET AFFECTATON -->
    <widget element="page[1]|page[2]|page[3]">
      <view>page frame</view>
    </widget>
    <widget element="page[1]/section[1]|page[1]/section[2]|page[1]/section[3]|page[2]/section[1]">
      <view>section frame</view>
    </widget>
    <widget element="page[1]/section[1]/cell[1]|page[1]/section[1]/cell[2]|page[1]/section[2]/cell[1]|page[1]/section[2]/cell[2]|page[1]/section[2]/cell[3]|page[1]/section[3]/cell[1]|page[2]/section[1]/cell[1]">
      <view>cell frame</view>
    </widget>
    <widget element="page[1]/section[1]/cell[1]/fragment[1]">
      <view>dam logo</view>
    </widget>
    <widget element="page[1]/section[1]/cell[2]/fragment[1]">
      <view>Nuxeo header</view>
    </widget>
    <widget element="page[1]/section[1]/cell[2]/fragment[2]">
      <view>dam includes</view>
    </widget>
    <widget element="page[1]/section[2]/cell[1]/fragment[1]">
      <view>dam search filters</view>
    </widget>
    <widget element="page[1]/section[2]/cell[2]/fragment[1]">
      <view>facelet region</view>
    </widget>
    <widget element="page[1]/section[2]/cell[3]/fragment[1]">
      <view>dam selection view</view>
    </widget>
    <widget element="page[1]/section[3]/cell[1]/fragment[1]">
      <view>Nuxeo footer</view>
    </widget>
    <widget element="page[2]/section[1]/cell[1]/fragment[1]">
      <view>facelet region</view>
    </widget>
    <!-- buttons -->
    <style name="default buttons">
      <selector path=".button">
        <font preset="default (DAM fonts)"/>
        <color>#333333</color>
        <border preset="buttons (DAM borders)"/>
        <background preset="buttons (DAM backgrounds)"/>
        <margin>5px 10px 10px 0px</margin>
        <padding>2px 5px</padding>
        <cursor>pointer</cursor>
        <border-radius>3px</border-radius>
      </selector>
      <selector path=".clipboard .button">
        <margin>1px 3px</margin>
        <padding>1px 5px</padding>
        <font-size>9px</font-size>
      </selector>
      <selector path=".clipboard a.button, .clipboard a.button:hover">
        <padding>2px 5px 2px 5px</padding>
        <text-decoration>none</text-decoration>
      </selector>
      <selector path=".button:hover">
        <color>#000000</color>
        <border-color>#92999e</border-color>
      </selector>
      <selector path=".button[disabled=disabled], .button[disabled=disabled]:hover">
        <color>#ccc</color>
      </selector>
      <selector path=".togglePanelTitleBox .button">
        <font preset="small (DAM fonts)"/>
        <margin>1px 5px 1px 0px</margin>
        <padding>1px 5px</padding>
      </selector>
      <selector path=".linkDisabled">
        <font preset="tiny (DAM fonts)"/>
        <color>#ccc</color>
        <border preset="buttons (DAM borders)"/>
        <background preset="buttons (DAM backgrounds)"/>
        <padding>2px 5px 2px 5px</padding>
        <margin>1px 3px</margin>
        <cursor>pointer</cursor>
        <border-radius>3px</border-radius>
      </selector>
      <selector path=".import">
        <font-size>12px</font-size>
        <font-weight>bold</font-weight>
        <color>#eee</color>
        <border>1px solid #000000</border>
        <background>none #4F4F4F</background>
        <padding>3px 8px</padding>
        <display>inline-block</display>
      </selector>
      <selector path=".import:hover">
        <text-decoration>none</text-decoration>
        <color>#fff</color>
        <background-color>#000</background-color>
        <border>1px solid #000</border>
      </selector>
      <selector path=".clipboard span span span, .clipboard span span input, .clipboard span span a">
        <float>right</float>
        <display>block</display>
      </selector>
    </style>
    <style name="header and footer style" inherit="default buttons">
      <selector path="a, a:link, a:visited">
        <text-decoration>none</text-decoration>
      </selector>
      <selector path="a:hover, a:active">
        <text-decoration>underline</text-decoration>
      </selector>
      <selector path="a, a:link, a:visited, a:hover, a:active">
        <color preset="header.link (DAM colors)" />
      </selector>
      <selector path="img">
        <margin-left>5px</margin-left>
        <margin-right>2px</margin-right>
        <vertical-align>middle</vertical-align>
      </selector>
      <selector path="form">
        <margin>0</margin>
        <padding>0</padding>
      </selector>
      <selector path=".userActions">
        <font preset="small (DAM fonts)"/>
        <color preset="header.text (DAM colors)" />
        <line-height>185%</line-height>
        <padding>5px 0 0 0</padding>
        <text-align>left</text-align>
        <float>left</float>
        <margin>0 0 0 15px</margin>
      </selector>
      <selector path=".userServicesBlock">
        <float>left</float>
        <padding>0</padding>
      </selector>
      <selector path=".userMenuActions">
        <font preset="default (DAM fonts)"/>
        <color preset="header.text (DAM colors)" />
        <line-height>185%</line-height>
        <padding>0px</padding>
        <text-align>left</text-align>
        <float>left</float>
        <margin>0 0 0 15px</margin>
      </selector>
      <selector path=".userMenuActions ul, .userMenuActions ul ul">
        <list-style>none</list-style>
      </selector>
      <selector path=".userMenuActions ul">
        <margin>0</margin>
        <padding>0</padding>
        <height>28px</height>
      </selector>
      <selector path=".userMenuActions ul li">
        <float>left</float>
        <text-align>left</text-align>
      </selector>
      <selector path=".userMenuActions ul li a:hover, .userMenuActions ul li:hover">
        <background-color preset="white (DAM backgrounds)" />
        <color preset="header.userMenuHover (DAM colors)" />
      </selector>
      <selector path=".userMenuActions a">
        <display>block</display>
        <text-decoration>none</text-decoration>
        <margin>0</margin>
        <color preset="header.userMenu (DAM colors)" />
        <background-color preset="white (DAM backgrounds)" />
      </selector>
      <selector path=".userMenuActions a.userButton">
        <background preset="header.userMenu (DAM backgrounds)"/>
        <padding>3px 8px 5px 23px</padding>
      </selector>
      <selector path=".userMenuActions li ul">
        <display>none</display>
        <position>absolute</position>
        <width>180px</width>
        <height>auto</height>
        <border preset="header.userMenuList (DAM borders)" />
        <box-shadow>3px 3px 5px rgba(0, 0, 0, 0.5)</box-shadow>
        <border-top>none</border-top>
        <z-index>1000</z-index>
      </selector>
      <selector path=".userMenuActions li:hover ul">
        <display>block</display>
      </selector>
      <selector path=".userMenuActions li ul a">
        <padding>4px 4px 4px 22px</padding>
      </selector>
      <selector path=".userMenuActions ul li li">
        <width>180px</width>
      </selector>
       <selector path=".userMenuActions ul ul a:hover">
        <background-color preset="header.userListLinkHover (DAM backgrounds)" />
      </selector>
      <selector path=".userMenuActions input.button">
        <margin>0 5px</margin>
      </selector>
      <selector path=".mainTabs">
        <font preset="default (DAM fonts)"/>
        <line-height>normal</line-height>
        <height>28px</height>
        <float>left</float>
      </selector>
      <selector path=".mainTabs ul">
        <margin>0</margin>
        <padding>0</padding>
      </selector>
      <selector path=".mainTabs li">
        <background-color preset="header.mainTabs (DAM backgrounds)" />
        <display>inline</display>
        <float>left</float>
        <height>28px</height>
        <margin>0 5px</margin>
      </selector>
      <selector path=".mainTabs a, .mainTabs a:visited">
        <color preset="header.mainTabs (DAM colors)" />
        <display>inline-block</display>
        <font-size>12px</font-size>
        <padding>5px 11px 8px</padding>
        <text-decoration>none</text-decoration>
      </selector>
      <selector path=".mainTabs a:hover">
        <color preset="header.mainTabsHover (DAM colors)" />
      </selector>
      <selector path=".mainTabs li.selected">
        <background-color preset="header.mainTabsSelected (DAM backgrounds)" />
      </selector>
      <selector path=".mainTabs li.selected a, .mainTabs li.selected a:hover, .mainTabs li.selected a:visited">
        <color preset="header.mainTabsSelected (DAM colors)" />
        <font-weight>bold</font-weight>
        <text-decoration>none</text-decoration>
      </selector>
      <selector path=".mainTabs li a:hover">
        <text-decoration>none</text-decoration>
      </selector>
      <selector path=".footerDefaultLinks">
        <border-top preset="footer (DAM borders)"/>
        <background preset="footer (DAM backgrounds)"/>
        <padding>15px</padding>
      </selector>
      <selector path=".footerBlock">
        <margin>0</margin>
        <line-height>1.25em</line-height>
        <padding>0</padding>
        <font preset="small (DAM fonts)"/>
      </selector>
      <selector path=".footerBlock div">
        <margin>0</margin>
        <line-height>1.25em</line-height>
        <padding>0</padding>
        <font preset="small (DAM fonts)"/>
      </selector>
      <selector path=".footerBlock form, .footerBlock p">
        <margin>5px 0px 0px 0px</margin>
        <padding>0</padding>
      </selector>
      <selector path=".nuxLinks">
        <float>right</float>
        <padding-left>20px</padding-left>
      </selector>
      <selector path=".nuxLinks a">
        <vertical-align>middle</vertical-align>
      </selector>
      <selector path=".copyrights">
        <float>right</float>
        <padding>2px 20px 0 0</padding>
        <color preset="footer.text (DAM colors)" />
      </selector>
      <selector path="a.twitt">
        <background preset="footer.twittLink (DAM backgrounds)" />
        <height>16px</height>
        <width>16px</width>
        <display>inline-block</display>
        <margin>0px 5px</margin>
      </selector>
      <selector path="a.twitt:hover">
        <background-position>left bottom</background-position>
      </selector>
      <selector path="a.linkedin">
        <background preset="footer.linkedInLink (DAM backgrounds)" />
        <height>16px</height>
        <width>16px</width>
        <display>inline-block</display>
        <margin>0px 5px</margin>
      </selector>
      <selector path="a.linkedin:hover">
        <background-position>-16px bottom</background-position>
      </selector>
      <selector path=".clear">
        <clear>both</clear>
      </selector>
    </style>
    <style name="common style" inherit="default buttons">
      <selector path="h1, h2, h3, h4, h5, h6, div, form, p, span">
        <margin>0</margin>
        <padding>0</padding>
      </selector>
      <selector path="body, div">
        <font preset="default (DAM fonts)"/>
        <color preset="blackfonts (DAM colors)"/>
        <line-height>1.25em</line-height>
      </selector>
      <selector path="img">
        <border>none</border>
        <margin>0</margin>
      </selector>
      <selector path="a">
        <color>#666666</color>
        <text-decoration>none</text-decoration>
      </selector>
      <selector path="a:visited, a:active">
        <color>#666666</color>
      </selector>
      <selector path="a:hover">
        <color>#666666</color>
        <text-decoration>underline</text-decoration>
      </selector>
      <selector path="a.external">
        <background preset="external (DAM backgrounds)"/>
        <padding-right>16px</padding-right>
      </selector>
      <selector path="input, table">
        <margin>0</margin>
      </selector>
      <selector path=".facesStatusMessage">
        <position>absolute</position>
        <left>48%</left>
        <top>10%</top>
      </selector>
      <selector path=".facesStatusMessage ul, .infoFeedback ul, .warningFeedback ul, warningMessage ul, .errorFeedback ul">
        <list-style-type>none</list-style-type>
        <width>200px</width>
        <padding>10px</padding>
      </selector>
      <selector path="table, .dataInput, .dataOutput, .dataList">
        <border-collapse>collapse</border-collapse>
        <font preset="default (DAM fonts)"/>
        <width>100%</width>
      </selector>
      <selector path=".dataList td">
        <padding>5px 3px</padding>
      </selector>
      <selector path=".dataOutput th">
        <border-bottom preset="graymedium (DAM borders)"/>
        <color>#000</color>
        <background preset="tableheader (DAM backgrounds)"/>
        <text-align>left</text-align>
        <height>18px</height>
        <font-weight>normal</font-weight>
      </selector>
      <selector path=".dataOutput td">
        <padding>5px 3px</padding>
      </selector>
      <selector path=".labelColumn">
        <width>125px</width>
        <vertical-align>top</vertical-align>
        <color preset="gray.medium (DAM colors)"/>
      </selector>
      <selector path=".fieldColumn">
        <padding-left>5px</padding-left>
        <white-space>normal</white-space>
        <color preset="black (DAM colors)"/>
      </selector>
      <selector path=".detail">
         <font preset="small (DAM Nyse fonts)"/>
      </selector>
      <selector path=".iconColumn">
        <width>20px</width>
        <text-align>center</text-align>
      </selector>
      <selector path=".searchInput">
        <background preset="searchInput (DAM backgrounds)"/>
        <border>1px solid #CCCCCC</border>
        <margin>0</margin>
        <padding>3px 3px 3px 20px</padding>
        <width>100%</width>
      </selector>
      <selector path=".action_bar">
        <margin>10px 10px 10px 0</margin>
        <line-height>normal</line-height>
      </selector>
      <selector path=".tabsBar">
        <width>100%</width>
        <line-height>normal</line-height>
      </selector>
      <selector path=".tabsBar ul">
        <margin>0</margin>
        <height>20px</height>
        <padding>0px 10px 0 0</padding>
        <list-style>none</list-style>
        <border-bottom preset="grayneutral (DAM borders)"/>
      </selector>
      <selector path=".tabsBar li">
        <margin>0</margin>
        <padding>0</padding>
        <float>left</float>
      </selector>
      <selector path=".tabsBar a, .tabsBar a:hover, .tabsBar a:visited">
        <color preset="blue.gray (DAM colors)"/>
        <border-right preset="grayneutral (DAM borders)"/>
        <border-bottom preset="grayneutral (DAM borders)"/>
        <background preset="tabs (DAM backgrounds)"/>
        <text-decoration>none</text-decoration>
        <font-weight>normal</font-weight>
        <display>block</display>
        <padding>3px 7px 0 7px</padding>
        <float>left</float>
        <height>17px</height>
      </selector>
      <selector path=".tabsBar a.selected, .tabsBar a.selected:hover, .tabsBar a.selected:visited">
        <color>#000</color>
        <border-right preset="grayneutral (DAM borders)"/>
        <border-bottom preset="white (DAM borders)"/>
        <background preset="white (DAM backgrounds)"/>
      </selector>
      <selector path=".tabsBar li a:hover">
        <color preset="blue.gray (DAM colors)"/>
        <background preset="white (DAM backgrounds)"/>
      </selector>
      <selector path="#selectionView iframe">
        <width>100%</width>
      </selector>
      <selector path=".tabsContent">
        <margin>0px</margin>
        <padding>5px 10px 10px 10px</padding>
      </selector>
      <selector path=".togglePanelTitleBox"/>
      <selector path=".togglePanel">
        <margin>0</margin>
        <background>#fff url(${org.nuxeo.ecm.contextPath}/img/data_title_bg.png) repeat-x top left</background>
      </selector>
      <selector path=".togglePanel h3">
        <padding>4px 0</padding>
        <font-size>11px</font-size>
        <float>left</float>
      </selector>
      <selector path=".togglePanel h3 .folded">
        <padding>0 0 0 18px</padding>
        <background>transparent url(${org.nuxeo.ecm.contextPath}/icons/arrow_folded.gif) top left no-repeat</background>
      </selector>
      <selector path=".togglePanel h3 .unfolded">
        <padding>0 0 0 18px</padding>
        <background>transparent url(${org.nuxeo.ecm.contextPath}/icons/arrow_unfolded.gif) top left no-repeat</background>
      </selector>
      <selector path=".togglePanel h3 a,.togglePanel h3 a:link, .togglePanel h3 a:visited, .togglePanel h3 a:hover, .togglePanel h3 a:visited:hover">
        <color>#4B4D68</color>
      </selector>
      <selector path=".togglePanel h3.noIcon">
        <padding>4px 0px 4px 10px</padding>
      </selector>
      <selector path=".dataBlockActions">
        <float>right</float>
        <padding>3px 0px</padding>
      </selector>
      <selector path=".togglePanel .togglePanelBody">
        <background>#ffffff</background>
        <padding>8px 10px</padding>
        <border-bottom>1px solid #ccc</border-bottom>
      </selector>
      <selector path=".togglePanel .togglePanelBody textarea">
        <padding>5px</padding>
        <font preset="default (DAM fonts)"/>
        <border>1px solid #E0E0E0</border>
      </selector>
      <selector path=".dataRowEven">
        <background preset="white (DAM backgrounds)"/>
      </selector>
      <selector path=".dataRowOdd">
        <background preset="blue.light (DAM backgrounds)"/>
      </selector>
      <selector path=".logopic">
        <padding>6px 5px 0px</padding>
        <font-size>1%</font-size><!-- for IE -->
      </selector>
      <selector path=".searchFilter"/>
      <selector path=".filterContent">
        <padding>0px</padding>
      </selector>
      <selector path=".banner_actions ul">
        <padding>0</padding>
        <margin>0</margin>
        <height>30px</height>
        <float>right</float>
        <list-style>none</list-style>
      </selector>
      <selector path=".banner_actions ul li">
        <float>left</float>
        <padding>0</padding>
      </selector>
      <selector path=".banner_actions ul li a">
        <display>block</display>
        <padding>9px 10px 8px</padding>
        <background>transparent url(${org.nuxeo.ecm.contextPath}/img/headerLink_bg.png) repeat-x top left</background>
      </selector>
      <selector path=".banner_actions ul li a:hover">
        <background>transparent url(${org.nuxeo.ecm.contextPath}/img/headerLink_bg_hover.png) repeat-x top left</background>
        <text-decoration>none</text-decoration>
      </selector>
      <selector path=".importButton">
        <padding>0 0 0 20px</padding>
        <background>url("/nuxeo/img/import_button.png") no-repeat scroll left bottom transparent</background>
      </selector>
      <selector path=".administrationButton">
        <padding>6px 0 3px 22px</padding>
        <background>transparent url(${org.nuxeo.ecm.contextPath}/img/administration_button.png) no-repeat top left</background>
      </selector>
      <selector path=".assetManagementButton">
        <padding>6px 0 3px 24px</padding>
        <background>transparent url(${org.nuxeo.ecm.contextPath}/img/asset_management_button.png) no-repeat top left</background>
      </selector>
      <selector path=".separator">
        <padding>9px 0px 8px 7px</padding>
        <display>block</display>
      </selector>
      <selector path=".userName">
        <padding>9px 4px 8px 0</padding>
        <display>block</display>
      </selector>
      <selector path=".logOutButton, .userName">
        <color>#afafaf</color>
      </selector>
      <selector path="a:hover .logOutButton">
        <color>#ffffff</color>
      </selector>
      <selector path=".pageNavigationControls">
        <padding>2px 5px</padding>
        <background preset="navigationbar (DAM backgrounds)"/>
        <text-align>center</text-align>
      </selector>
      <selector path=".previousBlock">
        <float>left</float>
        <width>15%</width>
        <text-align>left</text-align>
      </selector>
      <selector path=".statusBlock">
        <float>left</float>
        <width>70%</width>
        <padding-top>1px</padding-top>
      </selector>
      <selector path=".nextBlock">
        <float>left</float>
        <width>15%</width>
        <text-align>right</text-align>
      </selector>
      <selector path=".documentPreviewTitle">
        <padding>4px 5px</padding>
        <font-size>12px</font-size>
        <background preset="actionbar (DAM backgrounds)"/>
        <text-align>center</text-align>
        <border-bottom preset="grayneutral (DAM borders)"/>
      </selector>
      <selector path=".infoTabContent">
        <background-color>#6F6F6F</background-color>
        <width>410px</width>
      </selector>
      <selector path=".infoTabContent .assetContainer">
        <text-align>center</text-align>
        <vertical-align>middle</vertical-align>
        <height>388px</height>
        <width>410px</width>
        <margin>auto</margin>
      </selector>
      <selector path=".infoTabContent .assetContainer a">
        <width>410px</width>
        <text-align>center</text-align>
        <vertical-align>middle</vertical-align>
        <margin>auto</margin>
