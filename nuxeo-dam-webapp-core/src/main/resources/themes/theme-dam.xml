      <selector path="img">
        <border>none</border>
        <margin>0</margin>
      </selector>
      <selector path="a">
        <color>#666666</color>
        <text-decoration>none</text-decoration>
      </selector>
      <selector path="a:visited, a:active">
        <color>#666666</color>
      </selector>
      <selector path="a:hover">
        <color>#666666</color>
        <text-decoration>underline</text-decoration>
