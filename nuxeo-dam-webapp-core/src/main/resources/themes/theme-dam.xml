      <width>0%</width>
    </layout>
    <layout element="page[3]">
      <margin>0</margin>
      <padding>0</padding>
    </layout>
    <layout element="page[3]/section[1]">
      <width>100%</width>
    </layout>
    <layout element="page[3]/section[1]/cell[1]">
      <width>18%</width>
      <text-align>center</text-align>
    </layout>
    <layout element="page[3]/section[1]/cell[2]">
      <width>82%</width>
    </layout>
    <layout element="page[3]/section[2]">
      <height>100%</height>
      <width>100%</width>
    </layout>
    <layout element="page[3]/section[2]/cell[1]">
      <width>100%</width>
    </layout>
    <layout element="page[3]/section[3]">
      <width>100%</width>
